{
    "name": "sboo/laravel5-mailjet",
    "description": "Mailjet driver for Laravel 5",
    "keywords": ["laravel", "laravel 5", "mailjet", "api", "v3"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ramon Ackermann",
            "email": "ramon.ackermann@sboo.eu"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "5.5.*|5.6.*|5.7.*",
        "illuminate/mail": "5.5.*|5.6.*|5.7.*",
        "guzzlehttp/guzzle": "~5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Sboo\\Laravel5Mailjet\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sboo\\Laravel5Mailjet\\MailjetServiceProvider"
            ],
            "aliases": {
                "Mailjet": "Sboo\\Laravel5Mailjet\\Facades\\Mailjet"
            }
        }
    },
    "minimum-stability": "stable"
}
